    <script>pipeline {
  agent { label &apos;master&apos; }  // 指定在 master 節點執行

  environment {
    TZ = &apos;Asia/Taipei&apos;
  }

  parameters {
    string(name:&apos;PROJECT_NAME&apos; ,defaultValue:&apos;cclemon-ui&apos;)
    choice(name: &apos;PROFILE&apos;, choices: [&apos;dev&apos;, &apos;sit&apos; ,&apos;prod&apos;])
  }

  stages {
    stage(&apos;Deploy&apos;) {
      steps {
          script {
            def containerName = &quot;${params.PROJECT_NAME}-${params.PROFILE}&quot;
            def imageName = &quot;${params.PROJECT_NAME}:${params.PROFILE}&quot;

            echo &quot;Deploying ${containerName} using image ${imageName}&quot;

            // 安全保險：如果容器已存在，先刪除
            sh &quot;&quot;&quot;
              docker rm -f ${containerName} || true
            &quot;&quot;&quot;

            // 執行 docker run（你也可以加上 -p 或 -e 等參數）
            sh &quot;&quot;&quot;
              docker run -d \
                --name ${containerName} \
                --network cclemon-network \
                -p 5173:80 \
                ${imageName}
            &quot;&quot;&quot;
          }
        }
    }
  }
}
</script>
